; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="esg_game"
run/main_scene="res://scenes/pages/introduction_page.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://assets/Images/icons/icon.svg"

[autoload]

ScoreManager="*res://scripts/score_manager.gd"
GameManager="*res://scripts/game_manager.gd"
QuestionManager="*res://scripts/question_manager.gd"
SettingsManager="*res://scripts/settings_manager.gd"
TransitionScene="*res://scenes/pages/transition_scene.tscn"

[display]

window/size/viewport_width=648
window/size/viewport_height=1152
window/stretch/mode="canvas_items"
window/handheld/orientation=1

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
